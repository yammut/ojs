msgid "plugins.generic.openAIRE.displayName"
msgstr "OpenAIRE"

msgid "plugins.generic.openAIRE.description"
msgstr "The OpenAIRE plugin adds the ProjectID element to the article metadata and extends the OAI-PMH interface according to the OpenAIRE Guidelines 1.1, helping OJS journals to become OpenAIRE compliant."

msgid "plugins.generic.openAIRE.projectID"
msgstr "OpenAIRE ProjectID"

msgid "plugins.generic.openAIRE.projectID.description"
msgstr "If this research resulted from an EU initiative that complies with the <a href=\"https://www.openaire.eu/\" target=\"_blank\">OpenAIRE</a> metadata platform, please include the ProjectID, a six-digit number which corresponds with the Grant Agreement identifier."

msgid "plugins.generic.openAIRE.projectIDValid"
msgstr "Please enter a valid ProjectID (6 numbers)"
